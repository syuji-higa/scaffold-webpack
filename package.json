    "install": "^0.12.2",
    "npm": "^6.9.0",
    "prettier": "^1.16.4",
    "pug": "^2.0.3",
    "pug-html-loader": "^1.1.5",
    "pug-lint-loader": "0.0.4",
    "stlint": "^1.0.32",
    "stlint-loader": "^1.0.0",
    "stlint-v4fire": "^1.0.7",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
