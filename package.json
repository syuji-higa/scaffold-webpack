{
  "name": "scaffold-webpack",
  "version": "1.0.0",
  "description": "Scaffold of Webpack",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --mode=development --inline --hot",
    "prod": "NODE_ENV=production webpack --mode=production",
    "eslint": "eslint --ext .js --ignore-path .gitignore ./src/assets/js",
    "stlint": "stlint",
    "precommit": "npm run eslint && npm run stlint",
    "fix": "npm run eslint -- --fix && npm run stlint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syuji-higa/scaffold-webpack.git"
  },
  "author": "Syuji Higa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/syuji-higa/scaffold-webpack/issues"
  },
  "homepage": "https://github.com/syuji-higa/scaffold-webpack#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "extract-loader": "^3.1.0",
